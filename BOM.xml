            <forecast-period index="6" start-time-local="2021-06-12T00:00:00+10:00" end-time-local="2021-06-13T00:00:00+10:00" start-time-utc="2021-06-11T14:00:00Z" end-time-utc="2021-06-12T14:00:00Z">
