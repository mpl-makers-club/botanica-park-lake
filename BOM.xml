            <forecast-period index="7" start-time-local="2021-05-19T00:00:00+10:00" end-time-local="2021-05-20T00:00:00+10:00" start-time-utc="2021-05-18T14:00:00Z" end-time-utc="2021-05-19T14:00:00Z">
