            <forecast-period index="5" start-time-local="2021-04-15T00:00:00+10:00" end-time-local="2021-04-16T00:00:00+10:00" start-time-utc="2021-04-14T14:00:00Z" end-time-utc="2021-04-15T14:00:00Z">
