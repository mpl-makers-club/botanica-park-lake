            <forecast-period index="6" start-time-local="2021-07-16T00:00:00+10:00" end-time-local="2021-07-17T00:00:00+10:00" start-time-utc="2021-07-15T14:00:00Z" end-time-utc="2021-07-16T14:00:00Z">
