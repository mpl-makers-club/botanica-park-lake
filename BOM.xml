            <forecast-period index="4" start-time-local="2021-06-25T00:00:00+10:00" end-time-local="2021-06-26T00:00:00+10:00" start-time-utc="2021-06-24T14:00:00Z" end-time-utc="2021-06-25T14:00:00Z">
