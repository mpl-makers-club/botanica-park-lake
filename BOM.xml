            <forecast-period index="7" start-time-local="2021-05-16T00:00:00+10:00" end-time-local="2021-05-17T00:00:00+10:00" start-time-utc="2021-05-15T14:00:00Z" end-time-utc="2021-05-16T14:00:00Z">
