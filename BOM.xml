            </forecast-period>
            <forecast-period index="6" start-time-local="2021-07-10T00:00:00+10:00" end-time-local="2021-07-11T00:00:00+10:00" start-time-utc="2021-07-09T14:00:00Z" end-time-utc="2021-07-10T14:00:00Z">
