            </forecast-period>
            <forecast-period index="5" start-time-local="2021-07-23T00:00:00+10:00" end-time-local="2021-07-24T00:00:00+10:00" start-time-utc="2021-07-22T14:00:00Z" end-time-utc="2021-07-23T14:00:00Z">
