            <forecast-period index="4" start-time-local="2021-08-01T00:00:00+10:00" end-time-local="2021-08-02T00:00:00+10:00" start-time-utc="2021-07-31T14:00:00Z" end-time-utc="2021-08-01T14:00:00Z">
