            </forecast-period>
            <forecast-period index="2" start-time-local="2021-07-12T00:00:00+10:00" end-time-local="2021-07-13T00:00:00+10:00" start-time-utc="2021-07-11T14:00:00Z" end-time-utc="2021-07-12T14:00:00Z">
