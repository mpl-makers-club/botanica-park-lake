            <forecast-period index="7" start-time-local="2021-05-13T00:00:00+10:00" end-time-local="2021-05-14T00:00:00+10:00" start-time-utc="2021-05-12T14:00:00Z" end-time-utc="2021-05-13T14:00:00Z">
