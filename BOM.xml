            <forecast-period index="1" start-time-local="2021-09-04T00:00:00+10:00" end-time-local="2021-09-05T00:00:00+10:00" start-time-utc="2021-09-03T14:00:00Z" end-time-utc="2021-09-04T14:00:00Z">
